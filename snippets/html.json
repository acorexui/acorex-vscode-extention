{
  "ax-alert": {
    "prefix": "ax-alert",
    "body": [
      "<ax-alert color=\"${1:primary}\">",
      "   <ax-icon>${2:icon}</ax-icon>",
      "   <ax-title>${3:title}</ax-title>",
      "   <ax-content>${4:content}</ax-content>",
      "</ax-alert>"
    ]
  },
  "ax-button": {
    "prefix": "ax-button",
    "body": [
      "<ax-button color=\"${1:primary}\" text=\"${2:Button}\"></ax-button>"
    ]
  },
  "ax-button-group": {
    "prefix": "ax-button-group",
    "body": [
      "<ax-button-group color=\"${1:primary}\" (onItemClick)=\"${2:handleOnItemClick($event)}\">",
      "  <ax-button-item text=\"${3:Bold}\">",
      "    <ax-prefix>",
      "     <ax-icon icon=\"${4:}\"></ax-icon>",
      "    </ax-prefix>",
      "  </ax-button-item>",
      "</ax-button-group>"
    ]
  },
  "ax-menu": {
    "prefix": "ax-menu",
    "body": [
      "<ax-menu direction=\"${1:vertical}\" >",
      " <ax-menu-item>",
      "   <ax-item-menu text=\"${2:Menu Item}\"></ax-item-menu>",
      " </ax-item-menu>",
      " <ax-item-menu text=\"${3:Menu Item}\">",
      "   <ax-prefix>",
      "     <ax-icon>",
      "       <i class=\"${4:icon}\"></i>",
      "     </ax-icon>",
      "   </ax-prefix>",
      " </ax-menu-item> ",
      "</ax-menu>"
    ]
  },
  "ax-side-menu": {
    "prefix": "ax-side-menu",
    "body": [
      "<ax-side-menu>",
      " <ax-side-menu-item text=\"${1:Home}\">",
      "   <ax-prefix>",
      "     <ax-icon>",
      "       <i class=\"${2:icon}\"></i>",
      "     </ax-icon>",
      "   </ax-prefix>",
      " </ax-side-menu-item>",
      " <ax-side-menu-item text=\"${3:Product}\">",
      "   <ax-side-menu-item text=\"${4:Inside}\">",
      "   </ax-side-menu-item>",
      " </ax-side-menu-item>",
      "</ax-side-menu>"
    ]
  },
  "ax-check-box": {
    "prefix": "ax-check-box",
